digraph boxes_and_circles {

  # a 'graph' statement
  graph [overlap = true, 
        rankdir = LR,
        fontsize = 10]

  # 'node' statement

  node [shape = circle,
        fontname = Helvetica
        fixedsize = false] // sets as circles
        
  Fertilizer; Height; z

  # several 'edge' statements
 
  Fertilizer -> Height
  z -> {Fertilizer Height}
}